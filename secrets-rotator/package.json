{
  "name": "secrets-rotator",
  "version": "1.0.0",
  "description": "This is a basic version of secret rotation",
  "main": "./bin/index.js",
  "bin": "./bin/index.js",
  "private": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "test:integration": "jest --forceExit --coverage --verbose -w 1 --all tests/integration/",
    "test:unit": "jest --forceExit --coverage --verbose -w 1 --all tests/unit/",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix",
    "build": "yarn run tsc",
    "setup-husky-and-talisman": "bash scripts/setup-husky.sh && bash scripts/setup-talisman.sh",
    "run:talisman": "bash scripts/run-talisman.sh",
    "pre-commit": "yarn lint && yarn test:unit --watchAll=false && yarn run:talisman && circleci config validate",
    "pre-push": "yarn build",
    "prepare": "husky install"
  },
  "author": "Digital Innovation Labs",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.341.0",
    "@aws-sdk/client-secrets-manager": "^3.341.0",
    "@google-cloud/secret-manager": "^3.10.1",
    "@sendgrid/client": "^7.6.0",
    "ajv": "8.x.x",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.0",
    "ajv-keywords": "^5.0.0",
    "axios": "^0.24.0",
    "generate-password": "^1.6.1",
    "googleapis": "^91.0.0",
    "lodash": "^4.17.21",
    "yargs": "^17.2.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^2.7.3",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "axios-mock-adapter": "^1.20.0",
    "eslint": "7.29.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-jest-formatting": "3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "40.0.0",
    "husky": "^7.0.0",
    "jest": "^27.3.1",
    "jest-extended": "^1.2.0",
    "jest-when": "^3.4.2",
    "mock-argv": "^2.0.6",
    "nyc": "^15.1.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5",
    "typescript-eslint-parser": "^22.0.0",
    "aws-sdk-client-mock": "^2.1.1"
  },
  "resolutions": {
    "protobufjs": "7.2.4",
    "fast-xml-parser": "4.2.4"
  }
}
