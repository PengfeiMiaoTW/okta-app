{
  "source": {
    "name": "${SOURCE_NAME}",
    "passwordConfig": {
      "length": 24,
      "numbers": true,
      "symbols": false,
      "lowercase": true,
      "excludeSimilarCharacters": false,
      "exclude": "",
      "strict": false
    },
    "action": {
      "type": "GCP_SECRET_MANAGER_SOURCE",
      "secretName": "${GCP_SECRET_NAME}",
      "keyFieldName": "runtime.session.secret",
      "projectId": "${GCP_PROJECT_ID}",
      "authentication": {
        "type": "GCP_SERVICE_ACCOUNT",
        "scopes": [
          "test-scope"
        ],
        "credentials": "${GCLOUD_KEY}"
      }
    }
  },
  "consumers": []
}
