{
  "source": {
    "name": "Generate Password",
    "passwordConfig": {
      "length": 24,
      "numbers": true,
      "symbols": false,
      "lowercase": true,
      "excludeSimilarCharacters": false,
      "exclude": "",
      "strict": false
    },
    "action": {
      "type": "GENERATE_PASSWORD"
    }
  },
  "consumers": [
    {
      "name": "Test AWS Secret Manager ",
      "action": {
        "type": "AWS_SECRET_MANAGER_CONSUMER",
        "authentication": {
          "type": "AWS_ACCOUNT",
          "credentials": {
            "accessKeyId": "access key Id",
            "sessionToken": "aws session token"
          }
        }
      }
    }
  ]
}

